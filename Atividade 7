#include <stdio.h>

int main() {
    float salario, desconto;

    // Lê o salário do funcionário
    printf("Digite o salário do funcionário: ");
    scanf("%f", &salario);

    // Calcula 11% do salário
    desconto = salario * 0.11;

    // Verifica se o desconto é maior que o máximo permitido
    if (desconto > 318.20) {
        desconto = 318.20;
    }

    // Exibe o valor do desconto
    printf("O desconto previdenciário é: R$ %.2f\n", desconto);

    return 0;
}
